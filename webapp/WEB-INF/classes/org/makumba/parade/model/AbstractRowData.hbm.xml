<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
      "-//Hibernate/Hibernate Mapping DTD//EN"
      "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.makumba.parade.model">

	<class name="AbstractRowData" table="RowData">
		<id name="id" type="long" column="ROWDATA_ID">
        	<generator class="native"/>
    	</id>
	
	<property name="dataType" />
	<many-to-one name="row" column="id_row" not-null="true"/>
	
    
    <joined-subclass name="RowAnt" table="ROW_ANT">
        <key column="ROWDATA_ID"/>
        
        <property name="lastmodified" />
        <property name="buildfile" />
        
        <!--  
        <list name="topTargets" table="ROW_ANT_TOPTARGETS">
		    <cache usage="read-write" />
			<key><column name="ROWDATA_ID" not-null="false"/></key>
			<index column="POSITION" />
			<element type="string" column="topTargets" />
		</list>
		
		<list name="subTargets" table="ROW_ANT_SUBTARGETS">
		    <cache usage="read-write" />
			<key><column name="ROWDATA_ID" not-null="false"/></key>
			<index column="POSITION"/>
			<element type="string" column="subTargets" />
		</list>
		-->
		
       
    </joined-subclass>
    
    <joined-subclass name="RowTracker" table="ROW_TRACKER">
        <key column="ROWDATA_ID"/>
        
    </joined-subclass>
    
    <joined-subclass name="RowCVS" table="ROW_CVS">
        <key column="ROWDATA_ID"/>
        
        <property name="user" />
        <property name="module" />
        <property name="branch" />
        
    </joined-subclass>
    
    <joined-subclass name="RowWebapp" table="ROW_WEBAPP">
        <key column="ROWDATA_ID"/>
       
        <property name="contextname" />
        <property name="status" />
        
        
    </joined-subclass>
    
    <joined-subclass name="RowMakumba" table="ROW_MAKUMBA">
        <key column="ROWDATA_ID"/>
        
        <property name="version" />
        
    </joined-subclass>
    
</class>

</hibernate-mapping>
		
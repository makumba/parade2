<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
      "-//Hibernate/Hibernate Mapping DTD//EN"
      "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.makumba.parade.model">
	<class name="File" lazy="true">

		<cache usage="read-write" />

		<id name="id" column="file"><!-- unsaved-value="undefined" -->
			<generator class="native" />
		</id>

		<property name="name" />
		<property name="date" />
		<property name="size" />
		<property name="path" />
		<property name="parentPath" />
		<property name="isDir" />
		<property name="onDisk" />
		<property name="currentChars" />
		<property name="previousChars" />
		<property name="cvsURL" />
		<property name="cvsStatus" />
		<property name="cvsRevision" />
		<property name="cvsCheckedOutRevision" />
		<property name="cvsDate" />
		<property name="crawled">
		</property>

		<many-to-one name="row" column="row" not-null="true"
			outer-join="false" />

	</class>

</hibernate-mapping>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
      "-//Hibernate/Hibernate Mapping DTD//EN"
      "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.makumba.aether.model">
	<class name="ALE">

		<cache usage="read-write" />

		<id name="id" column="ale">
			<generator class="native" />
		</id>

		<property name="objectURL" />
		<property name="user" />
		<property name="focus">
        <column name="focus" default="0" />
        </property>
        <property name="nimbus">
        <column name="nimbus" default="0" />
        </property>
        <property name="virtualFocus">
        <column name="virtualFocus" default="0" />
        </property>
        <property name="virtualNimbus">
        <column name="virtualNimbus" default="0" />
        </property>
		
	</class>
</hibernate-mapping>



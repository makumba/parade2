<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
      "-//Hibernate/Hibernate Mapping DTD//EN"
      "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.makumba.aether.model">
	<class name="MatchedAetherEvent">

		<cache usage="read-write" />

		<id name="id" column="matchedaetherevent">
			<generator class="native" />
		</id>

		<property name="objectURL"/>
		<property name="objectType"/>
		<property name="actor"/>
		<property name="userType" />
		<property name="userGroup" />
		<property name="action"/>
		<property name="eventDate"/>
		<property name="initialLevelCoefficient"/>
		
		<many-to-one name="initialPercolationRule" class="InitialPercolationRule"/>

        <property name="virtualPercolation"/>

	</class>

</hibernate-mapping>